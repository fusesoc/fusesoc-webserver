[run]
omit =
    */migrations/*
    */tests/*
    */test_*.py
    */__init__.py
    */settings.py
    */wsgi.py
    */asgi.py
    */manage.py
    */venv/*
    */.venv/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __str__
    if self\.debug
    if settings\.DEBUG
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.: